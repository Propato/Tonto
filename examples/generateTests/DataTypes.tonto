package TestingDataTypes 
datatype RGB
{
    red: number
    green: number [1] {ordered}
    blue: number
}


datatype double

kind Car {
    cor: RGB [1] {ordered const}
    peso: double
}

subkind BMW
subkind Ferrari


enum Testando {
    TEST, 
    TEST2,
    TEST3
}

disjoint complete genset TesteGenset {
    general Car
    specifics BMW, Ferrari
}
